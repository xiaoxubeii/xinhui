{
  "$schema": "https://opencode.ai/config.json",
  "provider": {
    "xinhui": {
      "name": "xinhui",
      "npm": "@ai-sdk/openai",
      "options": {
        "baseURL": "https://code.ppchat.vip/v1",
        "apiKey": "{env:XINHUI_API_KEY}",
        "timeout": 30000
      },
      "models": {
        "xinhui-0.1": {
          "name": "xinhui-0.1",
          "id": "gpt-5.2"
        }
      }
    },
    "zhipu": {
      "name": "Zhipu",
      "npm": "@ai-sdk/openai-compatible",
      "options": {
        "baseURL": "https://open.bigmodel.cn/api/coding/paas/v4",
        "apiKey": "{env:ZHIPU_API_KEY}",
        "timeout": 30000
      },
    },
    "qwen": {
      "name": "Qwen",
      "npm": "@ai-sdk/openai-compatible",
      "options": {
        "baseURL": "https://dashscope.aliyuncs.com/compatible-mode/v1",
        "apiKey": "{env:QWEN_API_KEY}",
        "timeout": 30000
      },
      "models": {
        "qwen3-vl-plus": {
          "name": "Qwen3-VL Plus",
          "id": "qwen3-vl-plus",
          "attachment": true,
          "modalities": {
            "input": ["text", "image"],
            "output": ["text"]
          }
        }
      }
    }
  },
  "model": "opencode/minimax-m2.1-free",
  "small_model": "opencode/minimax-m2.1-free",
  "diet_vision_model": "qwen/qwen3-vl-plus",
  "mcp": {
    "xinhui-backend": {
      "type": "remote",
      "url": "http://127.0.0.1:8000/api/mcp?token={env:XINHUI_MCP_TOKEN}",
      "enabled": true
    }
  },
  "default_agent": "clinical",
}
